task.spawn(function()
    while true do
        if Window.Destroyed then break end
        task.wait(1)

        local money = game:GetService("Players").LocalPlayer["\229\128\188"]["\232\180\167\229\184\129"]["\233\135\145\229\184\129"].Value
        MoneyInfo:SetDesc(string.format(
            "Money: %s\n",
            money
        ))
    end
end)

Window:SelectTab(1);